# Frontend Environment Configuration

# NextAuth Configuration (REQUIRED for OAuth to work)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-key-here

# Google OAuth Configuration (REQUIRED for Google Sign-in)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Facebook OAuth Configuration (REQUIRED for Facebook Sign-in)
FACEBOOK_CLIENT_ID=your_facebook_client_id_here
FACEBOOK_CLIENT_SECRET=your_facebook_client_secret_here

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:4000

# Production API Configuration (uncomment for production)
# NEXT_PUBLIC_API_URL=https://your-backend-domain.com/api/v1

# Cashfree Configuration (for frontend integration)
NEXT_PUBLIC_CASHFREE_APP_ID=your_cashfree_app_id
NEXT_PUBLIC_CASHFREE_ENVIRONMENT=sandbox

# Production Cashfree Configuration (uncomment for production)
# NEXT_PUBLIC_CASHFREE_APP_ID=your_production_app_id
# NEXT_PUBLIC_CASHFREE_ENVIRONMENT=production

# Frontend URLs
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000
NEXT_PUBLIC_SUCCESS_URL=http://localhost:3000/order/success
NEXT_PUBLIC_FAILURE_URL=http://localhost:3000/order/failure

# Production Frontend URLs (uncomment for production)
# NEXT_PUBLIC_FRONTEND_URL=https://your-frontend-domain.com
# NEXT_PUBLIC_SUCCESS_URL=https://your-frontend-domain.com/order/success
# NEXT_PUBLIC_FAILURE_URL=https://your-frontend-domain.com/order/failure